{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"quotemark": [
			true,
			"single",
			"avoid-escape",
			"avoid-template"
		],
		"no-console": false,
		"ban-types": [
			false
		],
		"indent": [
			true,
			"spaces",
			2
		],
		"interface-name": [
			true,
			"never-prefix"
		],
		"no-magic-numbers": true,
		"no-var-keyword": true,
		"no-parameter-reassignment": true,
		"cyclomatic-complexity": [
			true,
			5
		],
		"typedef": [
			true,
			"call-signature",
			"arrow-call-signature",
			"parameter",
			"arrow-parameter",
			"property-declaration",
			"member-variable-declaration",
			"object-destructuring",
			"array-destructuring"
		]
	},
	"rulesDirectory": [
		"tslint-consistent-codestyle",
		"tslint-microsoft-contrib",
		"tslint-immutable"
	]
}
